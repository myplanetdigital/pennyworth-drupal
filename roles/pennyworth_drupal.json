{
  "name": "pennyworth_drupal",
  "default_attributes": {
  },
  "json_class": "Chef::Role",
  "env_run_lists": {
  },
  "run_list": [
    "recipe[php::module_curl]",
    "recipe[php::module_gd]",
    "recipe[php::module_mysql]",
    "recipe[drush::make]",
    "recipe[mysql::server]",
    "role[pennyworth]",
    "recipe[pennyworth_drupal]"
  ],
  "description": "Adds extra functionality for drupal on top of pennyworth, hopefully avoiding overriding customizations.",
  "chef_type": "role",
  "override_attributes": {
    "jenkins": {
      "mirror": "http://mirrors.jenkins-ci.org",
      "package_url": "http://pkg.jenkins-ci.org",
      "iptables_allow": "disable",
      "http_proxy": {
        "basic_auth_username": "jenkins",
        "basic_auth_password": "jenkins"
      }
    },
    "mysql": {
      "server_root_password": "root",
      "server_repl_password": "root",
      "server_debian_password": "root",
      "bind_address": "127.0.0.1"
    }
  }
}
